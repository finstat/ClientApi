<Window x:Class="DesktopFinstatApiTester.Windows.StatusWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DesktopFinstatApiTester.Windows"
        mc:Ignorable="d"
        Title="Generating" Height="100" Width="640" ResizeMode="NoResize" WindowStartupLocation="CenterOwner" Closed="Window_Closed" Closing="Window_Closing">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="100"/>
            <ColumnDefinition Width="200*"/>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="35"/>
            <RowDefinition Height="35"/>
        </Grid.RowDefinitions>
        <Label x:Name="labelGenerate" Content="Status:" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Margin="{DynamicResource Margin5}" />
        <Label x:Name="labelGenerateText" Content="" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Margin="{DynamicResource Margin5}" Grid.Column="1"/>
        <ProgressBar x:Name="progressBarGeenerate" HorizontalAlignment="Stretch" Height="Auto" Margin="{DynamicResource Margin5}" Grid.Row="1" VerticalAlignment="Stretch" Width="Auto" Grid.ColumnSpan="2" IsManipulationEnabled="True"/>
    </Grid>
</Window>
